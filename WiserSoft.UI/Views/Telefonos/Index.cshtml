@model IEnumerable<WiserSoft.UI.Models.Telefonos>
@{
    Layout = "~/Views/Shared/Master.cshtml";
}
<h2>Lista de Telefonos</h2>
<p>
    @Html.ActionLink("Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Numero)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Account_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Authtoken)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Numero)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Account_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Authtoken)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { Numero = item.Numero }) |
                @Html.ActionLink("Eliminar", "Delete", new { Numero = item.Numero })
            </td>
        </tr>
    }
</table>
